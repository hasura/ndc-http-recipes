            ]
          },
          {
            "Oauth2": [
              "https://uri.paypal.com/services/payments/realtimepayment"
            ]
          }
        ],
        "response": {
          "contentType": "application/json"
        }
      },
      "arguments": {},
      "description": "List web experience profiles",
      "result_type": {
        "element_type": {
          "name": "WebProfile",
          "type": "named"
        },
        "type": "array"
      }
    }
