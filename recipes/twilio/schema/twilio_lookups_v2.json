        "type": "boolean"
      }
    },
    "JSON": {
      "aggregate_functions": {},
      "comparison_operators": {},
      "representation": {
        "type": "json"
      }
    },
    "LookupsPhoneNumberEnumValidationError": {
      "aggregate_functions": {},
      "comparison_operators": {},
      "representation": {
        "one_of": [
          "TOO_SHORT",
          "TOO_LONG",
          "INVALID_BUT_POSSIBLE",
          "INVALID_COUNTRY_CODE",
          "INVALID_LENGTH",
          "NOT_A_NUMBER"
        ],
        "type": "enum"
      }
    },
    "String": {
      "aggregate_functions": {},
      "comparison_operators": {},
      "representation": {
        "type": "string"
      }
    },
    "URI": {
      "aggregate_functions": {},
      "comparison_operators": {},
      "representation": {
        "type": "string"
      }
    }
  }
}
