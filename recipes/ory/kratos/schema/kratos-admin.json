          },
          "rest": {
            "type": [
              "string"
            ]
          }
        },
        "expires_at": {
          "description": "ExpiresAt is the time (UTC) when the request expires. If the user still wishes to update the setting,\na new request has to be initiated.",
          "type": {
            "name": "TimestampTZ",
            "type": "named"
