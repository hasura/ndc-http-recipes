{
  "$schema": "https://raw.githubusercontent.com/hasura/ndc-http/refs/heads/main/ndc-http-schema/jsonschema/ndc-http-schema.schema.json",
  "settings": {
    "servers": [
      {
        "url": {
          "value": "https://api.sendgrid.com",
          "env": "SENDGRID_SERVER_URL"
        }
      }
    ],
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "header": "Authorization",
        "scheme": "bearer",
        "value": {
          "env": "SENDGRID_API_KEY"
        }
      }
    },
    "security": [
      {
        "BearerAuth": []
      }
    ],
    "version": "1.0.0"
  },
  "functions": {
    "GetAccountState": {
      "request": {
        "url": "/v3/partners/accounts/{accountID}/state",
        "method": "get",
        "response": {
          "contentType": "application/json"
        }
      },
      "arguments": {
        "accountID": {
          "description": "Twilio SendGrid account ID",
          "type": {
            "name": "String",
            "type": "named"
          },
          "http": {
            "name": "accountID",
            "in": "path",
            "schema": {
              "type": [
                "string"
              ]
            }
          }
        }
      },
      "description": "Get an account's state",
      "result_type": {
        "name": "AccountProvisioningStateRead",
        "type": "named"
      }
    },
    "ListAccount": {
      "request": {
        "url": "/v3/partners/accounts",
        "method": "get",
        "response": {
          "contentType": "application/json"
        }
      },
      "arguments": {
        "limit": {
          "description": "The number of items to return",
          "type": {
            "type": "nullable",
            "underlying_type": {
              "name": "Int32",
              "type": "named"
            }
          },
          "http": {
            "name": "limit",
            "in": "query",
            "schema": {
              "type": [
                "integer"
              ],
              "maximum": 100
            }
          }
        },
        "offset": {
          "description": "The last item successfully retrieved",
          "type": {
            "type": "nullable",
            "underlying_type": {
              "name": "String",
              "type": "named"
            }
          },
          "http": {
            "name": "offset",
            "in": "query",
            "schema": {
              "type": [
                "string"
              ]
            }
          }
        }
      },
      "description": "Get all accounts",
      "result_type": {
        "name": "ListAccountResult",
        "type": "named"
      }
    },
    "ListAccountOffering": {
      "request": {
        "url": "/v3/partners/accounts/{accountID}/offerings",
        "method": "get",
        "response": {
          "contentType": "application/json"
        }
      },
      "arguments": {
        "accountID": {
          "description": "Twilio SendGrid account ID",
          "type": {
            "name": "String",
            "type": "named"
          },
          "http": {
            "name": "accountID",
            "in": "path",
            "schema": {
              "type": [
                "string"
              ]
            }
          }
        }
      },
      "description": "Get account offerings",
      "result_type": {
        "name": "AccountProvisioningOfferingList",
        "type": "named"
      }
    },
    "ListOffering": {
      "request": {
        "url": "/v3/partners/offerings",
        "method": "get",
        "response": {
          "contentType": "application/json"
        }
      },
      "arguments": {},
      "description": "Get all available offerings",
      "result_type": {
        "name": "AccountProvisioningCatalog",
        "type": "named"
      }
    }
  },
